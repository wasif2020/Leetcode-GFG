//{ Driver Code Starts
// Initial Template for C++

#include <bits/stdc++.h>
using namespace std;

// } Driver Code Ends
// User function Template for C++

class Solution{
public:
    vector<int> minPartition(int N)
    {
      int curr[]{2000,500,200,100,50,20,10,5,2,1};
      vector<int>res;
      for(int i=0; i<10; i++){
          int c=N/curr[i];
          for(int j=0; j<c; j++){
              res.push_back(curr[i]);
          }
          N=N-c*curr[i];
          if(N==0){
              break;
          }
      }
      return res;
    }
};

//{ Driver Code Starts.

int main(){
    int t;
    cin>>t;
    while(t--){
        int N;
        cin>>N;
        
        Solution ob;
        vector<int> numbers = ob.minPartition(N);
        for(auto u: numbers)
            cout<<u<<" ";
        cout<<"\n";
    }
    return 0;
}
// } Driver Code Ends
